import java.util.*;
import java.lang.*;
import java.io.*;


 class VowelConsonant   
{
	public static void main (String[] args)
	{

    char ch='m';

        if(ch == 'a' || ch == 'e' || ch == 'i' || ch == 'o' || ch == 'u' )
            System.out.println(ch + " is vowel");
        else
            System.out.println(ch + " is consonant");
    }
    
	
}
